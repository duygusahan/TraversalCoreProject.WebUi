@model List<RoleAssingViewModel>
@{
    ViewData["Title"] = "AssignRole";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<div class="card mb-4">
    <h5 class="card-header">Roller</h5>
    <div class="card-body">
        <form>
             @for (int i = 0; i < Model.Count; i++)
            {
                <input type="hidden" asp-for="@Model[i].RoleId" />
                <input type="hidden" asp-for="@Model[i].RoleName" />
                <div class="form-check form-switch mb-2">
                    <input class="form-check-input" type="checkbox" id="flexSwitchCheckDefault" asp-for="@Model[i].RoleExist" />
                    <label class="form-check-label" for="@Model[i].RoleExist">@Model[i].RoleName</label>
                </div>
            }
            <button class="btn btn-danger">Kaydet</button>
        </form>
       
        
    </div>
</div>