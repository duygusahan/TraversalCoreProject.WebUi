@model TraversalCoreProject.EntityLayer.Concrete.Contact

<div class="col-lg-6 contact-left">
    <div class="partners">
        <div class="cont-details">
            <h5>İletişim Bilgilerimiz</h5>
            <p class="mt-3 mb-4">
                @Model.Description
            </p>
        </div>
        <div class="hours">
            <h6 class="mt-4">Mail Adresiniz:</h6>
            <p>
                <a href="mailto:mail@traversal.com">
                    @Model.Mail
                </a>
            </p>
            <h6 class="mt-4">Ofisimize Gelin:</h6>
            <p> @Model.Adress</p>
            <h6 class="mt-4">Telefon Numaramız:</h6>
            <p class="margin-top"><a href="tel:+44-255-366-88">@Model.Phone</a></p>
        </div>
    </div>
</div>