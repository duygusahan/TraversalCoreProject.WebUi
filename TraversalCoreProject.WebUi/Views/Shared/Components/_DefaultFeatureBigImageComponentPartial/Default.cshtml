@model List<TraversalCoreProject.EntityLayer.Concrete.FeatureBigImages>

@foreach (var item in Model)
{
    <div class="maghny-gd-1 col-lg-6">
        <div class="maghny-grid">
            <figure class="effect-lily border-radius  m-0">
                <img class="img-fluid" src="@item.BigImageUrl" alt="" style="height:800px" />
                <figcaption>
                    <div>
                        <h4>@item.BigImageTitle</h4>
                        <p>@item.BigImageDescription.Substring(0, 100)...</p>
                    </div>

                </figcaption>
            </figure>
        </div>
    </div>
}