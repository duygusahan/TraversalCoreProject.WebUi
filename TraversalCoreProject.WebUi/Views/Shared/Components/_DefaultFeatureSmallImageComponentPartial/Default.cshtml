@model List<TraversalCoreProject.EntityLayer.Concrete.FeatureSmallImages>

@foreach (var item in Model)
{
    <div class="maghny-gd-1 col-6">
        <div class="maghny-grid">
            <figure class="effect-lily border-radius">
                <img class="img-fluid" src="@item.SmallImageUrl" alt="" style="height:400px" />
                <figcaption>
                    <div>
                        <h4>@item.SmallImageTitle</h4>
                        <p>@item.SmallImageDescription.Substring(0, 50)... </p>
                    </div>

                </figcaption>
            </figure>
        </div>
    </div>

}