@model List<TraversalCoreProject.EntityLayer.Concrete.SubAbout>

@foreach (var item in Model)
{
    <section class="w3l-bottom py-5">
        <div class="container py-md-4 py-3 text-center">
            <div class="row my-lg-4 mt-4">
                <div class="col-lg-9 col-md-10 ml-auto">
                    <div class="bottom-info ml-auto">
                        <div class="header-section text-left">
                            <h4 class="hny-title two">@item.Title</h4>
                            <p class="mt-3 pr-lg-5">
                                @item.Description
                            </p>
                            <a href="about.html" class="btn btn-style btn-secondary mt-5">Turlar</a>
                        </div>


                    </div>
                </div>
            </div>
        </div>
    </section>
}