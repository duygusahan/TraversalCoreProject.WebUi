
@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("_MemberLayoutHeadComponentPartial")

<body class="g-sidenav-show  bg-gray-100">
    @await Component.InvokeAsync("_MemberLayoutNavbarComponentPartial")
    <main class="main-content position-relative max-height-vh-100 h-100 mt-1 border-radius-lg ">
        <!-- Navbar -->
        @await Component.InvokeAsync("_MemberLayoutNavbar2ComponentPartial")
    
        <!-- End Navbar -->
        <div class="container-fluid py-4">
            @RenderBody()
            @await Component.InvokeAsync("_MemberLayoutFooterComponentPartial")
        </div>
    </main>
    @await Component.InvokeAsync("_MemberLayoutRightbarComponentPartial")
    <!--   Core JS Files   -->
    @await Component.InvokeAsync("_MemberLayoutScriptsComponentPartial")
</body>

</html>
